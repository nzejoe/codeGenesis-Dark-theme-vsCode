      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#4FC1FF"
      }
    },
    {
      "name": "python function call",
      "scope": [
        "keyword.operator.comparison",
        "entity.name.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#cec58a"
      }
    },
    {
      "name": "python control function",
      "scope": "source",
      "settings": {
        "foreground": "#e4e4e4"
      }
    },
    {
      "name": "python  function parameters",
      "scope": [
        "variable.parameter",
        "keyword.operator.gradient.css",
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#f79375"
      }
    },

    {
      "name": "css",
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#eba24a"
      }
    },
    {
      "name": "css name.tag",
      "scope": "comment.block",
      "settings": {
        "foreground": "#4b4b4b"
      }
    },
    {
      "name": "css functions",
      "scope": [
        "meta.function.gradient.css",
        "support.function.shape.css",
        "support.function.transform.css",
        "support.function.misc.css",
        "support.function.url.css"
      ],
      "settings": {
        "foreground": "#65acc4"
      }
    },
    {
      "name": "css units & values",
      "scope": [
        "meta.property-value.css",
        "keyword.other.unit.vh.css",
        "constant.numeric.css",
        "keyword.other.unit.rem.css",
        "keyword.other.unit.percentage.css",
        "keyword.other.unit.s.css"
      ],
      "settings": {
        "foreground": "#FFA6C1"
      }
    },
    {
      "name": "react units",
      "scope": [
        "entity.other.attribute-name",
        "variable.other.property",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#ac7dad"
      }
    },
    {
      "name": "js function call",
      "scope": [
        "support.class.promise.js",
        "support.class.component",
        "source.python",
        "punctuation.definition.entity",
        "meta.property-list.css"
      ],
      "settings": {
        "foreground": "#71beb5"
      }
    },
    {
      "name": "js function call",
      "scope": [
        "keyword.operator.comparison",
        "storage.modifier.async",
        "keyword.control.flow",
        "entity.name.tag.html",
        "entity.name.tag.css",
        "entity.name.tag.wildcard.css"
      ],
      "settings": {
        "foreground": "#EF476F"
      }
    },
    {
      "name": "js function call",
      "scope": ["text.html.derivative", "variable"],
      "settings": {
        "foreground": "#e7e7e7"
      }
    },
    {
      "name": "storage.type",
      "scope": [
        "entity.name.function.decorator",
        "keyword.operator.logical",
        "meta.function.decorator"
      ],
      "settings": {
        "foreground": "#569CD6"
      }
